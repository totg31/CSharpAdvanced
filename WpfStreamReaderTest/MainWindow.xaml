<Window x:Class="WpfStreamReaderTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfStreamReaderTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="BtnFile" Content="File Click" Click="BtnFile_Click"></Button>
        <Button x:Name="BtnNumbers" Content="Numbers Click" Click="BtnNumbers_Click"></Button>
    </StackPanel>
</Window>
